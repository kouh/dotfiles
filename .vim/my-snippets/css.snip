snippet reset
alias rst
  * {
    box-sizing: border-box;
    margin: 0;
    padding: 0;
    -webkit-text-size-adjust: 100%;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  }

  button {
    user-select: none;
  }

  img {
    max-width: 100%;
    vertical-align: middle;
  }

  table {
    border-spacing: 0;
    border-collapse: collapse;
  }
snippet animation-keyframe
alias key keyframe
  @keyframes ${1:name} {
    ${2:0}%{
      ${3}
    }
  }
