include javascript.snip
include css.snip
snippet html
alias !
  <!DOCTYPE html>
  <html lang="ja">
  <head>
    <meta charset="UTF-8">
    <title></title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
  </head>
  <body>
    ${1}
  </body>
  </html>

snippet fakeimg
alias fake fki
  <img src="http://placehold.jp/${1:150}x${2:150}.png">

snippet script
alias js
  <script src="${1}"></script>

snippet link
alias css
  <link rel="${1:stylesheet}" href="${2}">
